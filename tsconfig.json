{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "checkJs": false,
    "jsx": "react-jsx",
    "lib": ["DOM", "WebWorker", "ES2017"],
    "module": "ESNext",
    "target": "ES2017",
    "moduleResolution": "node",
    "noPropertyAccessFromIndexSignature": false,
    "outDir": "./dist/",
    "plugins": [{
      "name": "typescript-plugin-css-modules",
      "options": {
        "customMatcher": "\\.css$"
      }
    }],
  }
}
